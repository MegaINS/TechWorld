<configuration>

    <logger name="ch" level="OFF"/>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <Pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %p %c{0}:%L %m%n
            </Pattern>
        </encoder>
    </appender>
       <root level="INFO">
        <appender-ref ref="STDOUT" />
        <appender-ref ref="netexamlog" />
    </root>
</configuration>